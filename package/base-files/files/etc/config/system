config system
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option hostname 'sanitariu'
	option zonename 'Europe/Sofia'
	option timezone 'EET-2EEST,M3.5.0/3,M10.5.0/4'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config led
	option name 'wlan'
	option sysfs 'green:wlan'
	option trigger 'netdev'
	option dev 'wlan0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'wan'
	option trigger 'switch0'
	option port_mask '0x1'
	option sysfs 'green:power'

config led
	option sysfs 'green:lan'
	option default '0'
	option trigger 'switch0'
	option port_mask '0x1e'
	option name 'lan'
